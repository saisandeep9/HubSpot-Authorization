doctype html
html( lang = "en")
body
    if token
        p your token is: #{token}
        p data :#{contacts}
        p your contacts:
            table
                tr
                    th firstname
                    th last name
                    th email
                   
                
                each contact in contacts  
                 tr 
                  td #{contact.properties.firstname.value}
                  td #{contact.properties.lastname.value}
                  td #{contact}
                  each identity-profile in identity-profiles
                  #{identity-profile.value}
                
                
    else
        h1 please click #[a(href=`${authUrl}`) here] to install
    


